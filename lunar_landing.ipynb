    "#Run and train model\n",
    "import time\n",
    "\n",
    "start = time.time()\n",
    "print('start time: ',start)\n",
    "train_rewards = []\n",
    "NN = Train()\n",
    "NN.update(model,loss_fn,optimizer,train_rewards)\n",
    "end = time.time()\n",
    "print('end time: {} \\n time elapsed: {}'.format(end, end-start))"
   ]
